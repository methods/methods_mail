<title><%= @user.name %></title>
<% @OutputName = [] %>
<% @OutputName = @user.name.split(" ") %>
<h1>Welcome <%= @OutputName[0] %></h1>
<br>
<br>
<br>
<h3>Contacts</h3>

<% @user.contacts.each do |contact|%>
<% if contact.group_name == "default" %>
	<%= contact.name  %>
	<%= contact.email %>
	<% end %>
<% end %>
<br>
<br>
<br>
<br>
<br>
<br>
<%= link_to 'New contact', new_user_contact_path(@user) %>
