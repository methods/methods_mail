<title>New group</title>
<h1>Create new group</h1>

<br>
<br>
<br>
<%= form_for :contact, url: user_contacts_path(@user) do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :email %>
<%= f.text_field :email %>
<br>
<%= f.label :group %>
<%= f.text_field :group_name%>

<br>
<%= f.submit "Create contact" %>
<% end %>
